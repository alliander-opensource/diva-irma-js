    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-mocha": "5.2.0",
    "eslint-plugin-react": "7.4.0"
  },
  "dependencies": {
    "bluebird": "3.5.1",
    "chai": "4.1.2",
